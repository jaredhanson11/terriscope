{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "api debugger",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ]
    }
  ],
  "compounds": [
    {
      "name": "Attach Debuggers",
      "configurations": ["api debugger"]
    }
  ]
}
