{
  "name": "terriscope",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm openapi:generate:watch & vite",
    "build": "tsc -b && vite build",
    "openapi:generate": "npx openapi-typescript ../api/openapi.json -o ./src/lib/api/v1.d.ts",
    "openapi:generate:watch": "nodemon --watch ../api/openapi.json --exec 'pnpm openapi:generate'",
    "lint:fix": "pnpm lint:format:fix && pnpm lint:eslint:fix && pnpm lint:build:check",
    "lint:check": "pnpm lint:format:check && pnpm lint:eslint:check && pnpm lint:build:check",
    "lint:format:check": "prettier . --check",
    "lint:format:fix": "prettier . --check --write",
    "lint:eslint:check": "eslint",
    "lint:eslint:fix": "eslint --fix",
    "lint:build:check": "tsc -b --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@fontsource-variable/noto-sans": "^5.2.10",
    "@tabler/icons-react": "^3.36.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query": "^5.90.12",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-config-prettier": "^10.1.8",
    "formik": "^2.4.9",
    "input-otp": "^1.4.2",
    "maplibre-gl": "^5.15.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript-helpers": "^0.0.15",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
    "react-resizable-panels": "^4.0.15",
    "react-router-dom": "^7.11.0",
    "recharts": "2.15.4",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-svgr": "^4.5.0"
  }
}
